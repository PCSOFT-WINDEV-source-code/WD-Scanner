     procedure_id : 1288120396414273658
     type_code : 14
     code : |1+
      procedure DisplayOptionsByDestination() <IHM>
      
      // Panes are displayed according to the destination
      switch COMBO_SCAN_TYPE
      	case cnDestinationPDFFile
      		LAYOUT_OPTIONS[cnQualityPane]..VisibleWithAnimation	= False
      		LAYOUT_OPTIONS[cnColorPane]..VisibleWithAnimation	= False		
      	case cnDestinationBMPFile
      		LAYOUT_OPTIONS[cnQualityPane]..VisibleWithAnimation	= False
