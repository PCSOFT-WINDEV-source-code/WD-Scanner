      procedure sExtensionByDestination(sFileName):chane
      
      switch COMBO_SCAN_TYPE
      	case cnDestinationBMPFile
      		return fExtractPath(sFileName,fDrive+fFileName+fDirectory)+".bmp"
      	case cnDestinationGIFFile
      		return fExtractPath(sFileName,fDrive+fFileName+fDirectory)+".gif"
      	case cnDestinationJPGFile
      		return fExtractPath(sFileName,fDrive+fFileName+fDirectory)+".jpg"
      	case cnDestinationPDFFile
      		return fExtractPath(sFileName,fDrive+fFileName+fDirectory)+".pdf"
      	other case
      		return sFileName
      end
