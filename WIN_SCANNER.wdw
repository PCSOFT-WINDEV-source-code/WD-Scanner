  procedures :
   -
     name : Digitizing
     procedure_id : 1288120396414142586
     type_code : 14
     code : |1-
      procedure Digitizing():boolen
      
      // Local variables
      MyImage is Image
      
      // Take the parameters defined by the user into account
      ManageDigitizingParameters()
      
      // The acquisition format changes according to the tab
      switch COMBO_SCAN_TYPE
      	case cnDestinationPDFFile			// PDF
      		ScanToPDF(gMyScanner,scanNumberPagesAll,EDT_DESTINATION_FILE)
      		
      	case cnDestinationBMPFile : 		// Bitmap		
      		MyImage = ScanToImage(gMyScanner)
      		MyImage.SaveBMP(EDT_DESTINATION_FILE)
      		
      	case cnDestinationGIFFile : 		// GIF			
      		MyImage = ScanToImage(gMyScanner)
      		MyImage.SaveGIF(EDT_DESTINATION_FILE,COMBO_NUMBER_COLORS.DisplayedValue)
      		
      	case cnDestinationJPGFile : 		// JPEG
      		MyImage = ScanToImage(gMyScanner)
      		MyImage.SaveJPEG(EDT_DESTINATION_FILE,Val(COMBO_QUALITY))			
      end
      
      // Display the result
      IMG_PREVIEW = 	EDT_DESTINATION_FILE
      
      // Manage the errors
      if ErrorOccurred then	
      	return False
      end
      
      // The digitizing process is performed properly by default
      return True
     type : 458752
   -
     name : ManageDigitizingParameters
     internal_properties : CAAAAAgAAAAuH4H5xxKAwebq98BXwcNm+ME2RfpXoWSl+wrUEFFgRy4rOfk/AzEUUQ5TgfAYDiHc+1VwOnQpmNcP9JZXPokMSW8jDIBeF7p3SPb5Q55nbLtwC65VsI+EhugN5vs2t3YjuOLJyufaAd4iCpH1
     procedure_id : 1288120396414208122
     type_code : 14
     code : |1-
      procedure ManageDigitizingParameters()
      
      // Image resolution
      gMyScanner.Resolution = COMBO_RESOLUTION
      
      // Scanner source
      switch COMBO_SOURCE.Select()
      	case 1
      		gMyScanner.ScanFrom = scanFromAuto
      	case 2
      		gMyScanner.ScanFrom = scanFromFeeder
      	case 3		
      		gMyScanner.ScanFrom = scanFromFlatBed	
      end
      
      // Image format
      switch COMBO_FORMAT.Select()
      	case 1 : // According to the device
      		gMyScanner.ColorFormat = scanColorFormatAuto 
      	case 2 : // RGB Mode
      		gMyScanner.ColorFormat = scanColorFormatRGB  
      	case 3 : // Grayscale
      		gMyScanner.ColorFormat = scanColorFormatGrayscale   
      	case 4 : // Black and White
      		gMyScanner.ColorFormat = scanColorFormatBlackWhite    
      		
      end
      
      // Other parameters are left with the default values
     type : 458752
